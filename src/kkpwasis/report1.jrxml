	<queryString>
		<![CDATA[SELECT
     transaksi.`kd_trx` AS transaksi_kd_trx,
     transaksi.`tgl` AS transaksi_tgl,
     transaksi.`nm_plg` AS transaksi_nm_plg,
     transaksi.`total` AS transaksi_total,
     transaksi.`diskon` AS transaksi_diskon,
     transaksi.`bayar` AS transaksi_bayar,
     transaksi.`kembali` AS transaksi_kembali,
     trx_plg.`kd_menu` AS trx_plg_kd_menu,
     trx_plg.`jns_menu` AS trx_plg_jns_menu,
     trx_plg.`nm_menu` AS trx_plg_nm_menu,
     trx_plg.`hrg_menu` AS trx_plg_hrg_menu,
     trx_plg.`banyak` AS trx_plg_banyak
FROM
     `trx_plg` trx_plg INNER JOIN `transaksi` transaksi ON trx_plg.`kd_trx` = transaksi.`kd_trx`
WHERE
	transaksi.`tgl` = $P{tanggal}]]>

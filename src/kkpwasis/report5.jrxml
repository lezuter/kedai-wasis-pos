	<queryString>
		<![CDATA[SELECT
     date_fotmat(transaksi.`tgl`,'%M')AS bulan,
     transaksi.`kd_trx` AS transaksi_kd_trx,
     transaksi.`tgl` AS transaksi_tgl,
     transaksi.`nm_plg` AS transaksi_nm_plg,
     transaksi.`total` AS transaksi_total,
     transaksi.`diskon` AS transaksi_diskon,
     transaksi.`bayar` AS transaksi_bayar,
     transaksi.`kembali` AS transaksi_kembali
FROM
     `transaksi` transaksi
WHERE
	month(tgl) = $P{pbulan} and year(tgl) = $P{ptahun}]]>
